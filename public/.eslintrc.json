{
    "env": {
        "es6": true,
        "browser": true,
        "node": false
    },
    "extends": "eslint:recommended",
    "rules": {
        "indent": [ "warn", 4, {
            "SwitchCase": 1
        } ],
        "linebreak-style": [ "error", "unix" ],
        "quotes": [ "error", "single" ],
        "semi": [ "error", "always" ],
        "no-console": 0
    }
}
